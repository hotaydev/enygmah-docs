    "@headlessui/react": "^1.7.15",
    "autoprefixer": "^10.4.8",
    "clsx": "^1.2.1",
    "dedent": "^0.7.0",
    "framer-motion": "^6.5.1",
    "postcss": "^8.4.16",
    "prism-react-renderer": "^1.3.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.11.0",
    "react-share": "^4.4.0",
    "tailwindcss": "^3.3.5",
